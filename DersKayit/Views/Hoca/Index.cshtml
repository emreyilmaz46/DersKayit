@model IEnumerable<DersKayit.Models.Ders>

@{
    ViewBag.Title = "Öğretim Elemanı İşlemleri";
}

<h2>Öğretim Elemanı İşlemleri</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>@item.DersAdi</li>
        <ol>
        @foreach (var ogr in item.KayitliOgrenciler)
        {
            <li> @ogr.OgrenciTamAd </li>
        }
        </ol>
    }
</ul>

